{
  "name": "@storybook/linter-config",
  "version": "2.0.0",
  "private": false,
  "description": "The config storybook uses to lint code",
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "storybook"
  ],
  "homepage": "https://storybook.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/eslint-config.git"
  },
  "license": "ISC",
  "author": "Norbert de Langen",
  "files": [
    "README.md",
    "eslint.config.js",
    "index.js",
    "remark.config.js",
    "prettier.config.js"
  ],
  "main": "eslint.config.js",
  "eslintConfig": {
    "extends": [
      "./eslint.config.js"
    ]
  },
  "scripts": {
    "test": "eslint --ext .js,.jsx,.json,.html,.ts,.tsx,.mjs --report-unused-disable-directives ./test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-json-files": "^0.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.16.0",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-recommended": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "prettier": "^1.19.1",
    "react": "*",
    "remark": "^11.0.2",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "eslint": "^5.0.0",
    "prettier": "^1.0.0",
    "remark": "^11.0.0",
    "typescript": "^3.4.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
