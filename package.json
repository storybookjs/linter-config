{
  "name": "@storybook/linter-config",
  "version": "4.0.0",
  "private": false,
  "type": "module",
  "description": "The config storybook uses to lint code",
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "storybook"
  ],
  "homepage": "https://storybook.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/linter-config.git"
  },
  "license": "ISC",
  "author": "Norbert de Langen",
  "files": [
    "README.md",
    "eslint.config.js",
    "index.js",
    "remark.config.js",
    "prettier.config.js"
  ],
  "main": "index.js",
  "eslintConfig": {
    "extends": [
      "./eslint.config.cjs"
    ],
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  },
  "scripts": {
    "test": "npm run test:eslint && npm run test:conflicts",
    "test:eslint": "eslint --ext .js,.jsx,.json,.html,.ts,.tsx,.mjs --report-unused-disable-directives ./test",
    "test:conflicts": "eslint-config-prettier eslint.config.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-files": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-progress": "^0.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.42",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react": "18.2.0",
    "remark": "^13.0.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "remark": "^12.0.0 || ^13.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
