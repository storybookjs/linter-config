{
  "name": "@storybook/linter-config",
  "version": "3.2.0",
  "private": false,
  "description": "The config storybook uses to lint code",
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "storybook"
  ],
  "homepage": "https://storybook.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/purepm/linter-config.git"
  },
  "license": "ISC",
  "author": "Norbert de Langen",
  "files": [
    "README.md",
    "eslint.config.js",
    "index.js",
    "remark.config.js",
    "prettier.config.js"
  ],
  "main": "index.js",
  "eslintConfig": {
    "extends": [
      "./eslint.config.js"
    ],
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  },
  "scripts": {
    "test": "npm run test:eslint && npm run test:conflicts",
    "test:eslint": "eslint --ext .js,.jsx,.json,.html,.ts,.tsx,.mjs --report-unused-disable-directives ./test",
    "test:conflicts": "eslint-config-prettier eslint.config.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-json-files": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-progress": "^0.0.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "remark-cli": "^10.0.1",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "react": "17.0.2",
    "remark": "^14.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "remark": "^12.0.0 || ^13.0.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
