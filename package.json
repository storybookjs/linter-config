{
  "name": "@storybook/linter-config",
  "version": "2.5.0",
  "private": false,
  "description": "The config storybook uses to lint code",
  "keywords": [
    "config",
    "eslint",
    "prettier",
    "storybook"
  ],
  "homepage": "https://storybook.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/eslint-config.git"
  },
  "license": "ISC",
  "author": "Norbert de Langen",
  "files": [
    "README.md",
    "eslint.config.js",
    "index.js",
    "remark.config.js",
    "prettier.config.js"
  ],
  "main": "eslint.config.js",
  "eslintConfig": {
    "extends": [
      "./eslint.config.js"
    ]
  },
  "scripts": {
    "test": "eslint --ext .js,.jsx,.json,.html,.ts,.tsx,.mjs --report-unused-disable-directives ./test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-json-files": "^0.8.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "react": "17.0.1",
    "remark": "^13.0.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "eslint": "^6.0.0",
    "prettier": "^2.0.0",
    "remark": "^12.0.0",
    "typescript": "^3.8.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
